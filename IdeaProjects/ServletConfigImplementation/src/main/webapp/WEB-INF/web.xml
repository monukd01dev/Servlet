<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>com.servlet.MyServlet</servlet-class>
    <init-param>
      <param-name>number</param-name>
<!--      <param-value>one</param-value>-->
      <param-value>two</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/MyServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MyServlet2</servlet-name>
    <servlet-class>com.servlet.MyServlet2</servlet-class>
    <init-param>
      <param-name>number</param-name>
      <param-value>one</param-value>
    </init-param>
    <init-param>
      <param-name>number1</param-name>
      <param-value>two</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet2</servlet-name>
    <url-pattern>/MyServlet2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ServletContextDemo</servlet-name>
    <servlet-class>com.servlet.ServletContextDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletContextDemo</servlet-name>
    <url-pattern>/ServletContextDemo</url-pattern>
  </servlet-mapping>

  <context-param>
    <param-name>uncanny</param-name>
    <param-value>meme.mp4</param-value>
  </context-param>
 <context-param>
    <param-name>message</param-name>
    <param-value>Learning Java From January To ...</param-value>
  </context-param>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
